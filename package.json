{
  "name": "@ogea12/icons",
  "description": "Librairie d'icÃ´nes simple et intuitive pour React",
  "version": "1.1.20",
  "engines": {
    "node": ">=20.17.0"
  },
  "type": "module",
  "files": [
    "build"
  ],
  "exports": {
    ".": {
      "types": "./build/icons/index.d.ts",
      "import": "./build/icons/index.js",
      "require": "./build/icons/index.cjs"
    },
    "./factory": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js",
      "require": "./build/index.cjs"
    }
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "prebuild": "node ./scripts/compile.js && npm run format && npm run lint:fix",
    "build": "tsup",
    "postbuild": "del-cli build/**/*.d.cts",
    "prepublishOnly": "npm run build",
    "release": "npx release-it",
    "taze": "npx taze",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "homepage": "https://github.com/ogea12/icons#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ogea12/icons.git"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@ogea12/commitlint-config": "^1.0.4",
    "@ogea12/eslint-config": "^2.0.5",
    "@ogea12/prettier-config": "^1.0.7",
    "@ogea12/tsconfig": "^1.0.8",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@swc/core": "^1.12.9",
    "@types/node": "^22.16.0",
    "@types/react": "^18.3.23",
    "change-case": "^5.4.4",
    "del-cli": "^6.0.0",
    "eslint": "^9.30.1",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "release-it": "^19.0.3",
    "storybook": "^8.6.14",
    "svg-parser": "^2.0.4",
    "taze": "^19.1.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "author": "OGEA 12",
  "license": "MIT",
  "keywords": [
    "ogea12",
    "icons"
  ],
  "prettier": "@ogea12/prettier-config",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): ${version}",
      "tagAnnotation": "v${version}",
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}",
      "web": true
    }
  }
}
