{
  "name": "@ogea12/icons",
  "description": "Librairie d'icÃ´nes simple et intuitive pour React",
  "version": "2.0.12",
  "engines": {
    "node": ">=20.17.0"
  },
  "type": "module",
  "files": [
    "build"
  ],
  "exports": {
    ".": {
      "types": "./build/icons/index.d.ts",
      "import": "./build/icons/index.js",
      "require": "./build/icons/index.cjs"
    },
    "./factory": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js",
      "require": "./build/index.cjs"
    }
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "prebuild": "node ./scripts/compile.js && npm run format && npm run lint:fix",
    "build": "tsup",
    "postbuild": "del-cli build/**/*.d.cts",
    "prepublishOnly": "npm run build",
    "release": "release-it",
    "taze": "taze",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "homepage": "https://github.com/ogea12/icons#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ogea12/icons.git"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@ogea12/commitlint-config": "^1.0.6",
    "@ogea12/eslint-config": "^2.1.1",
    "@ogea12/prettier-config": "^1.0.9",
    "@ogea12/tsconfig": "^1.0.12",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/addon-vitest": "^9.1.3",
    "@storybook/react-vite": "^9.1.3",
    "@swc/core": "^1.13.5",
    "@types/node": "^22.18.0",
    "@types/react": "^19.1.11",
    "@types/svg-parser": "^2.0.6",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "change-case": "^5.4.4",
    "del-cli": "^6.0.0",
    "eslint": "^9.34.0",
    "husky": "^9.1.7",
    "playwright": "^1.55.0",
    "prettier": "^3.6.2",
    "release-it": "^19.2.2",
    "storybook": "^9.1.3",
    "svg-parser": "^2.0.4",
    "taze": "^19.3.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "markdown-to-jsx": "^7.7.13"
  },
  "peerDependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "author": "OGEA 12",
  "license": "MIT",
  "keywords": [
    "ogea12",
    "icons"
  ],
  "prettier": "@ogea12/prettier-config",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): ${version}",
      "tagAnnotation": "v${version}",
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}",
      "web": true
    }
  }
}
